	"version": "0.2.0",
	"configurations": [
		{
			"name": "Attach Muse WS",
			"type": "node",
			"request": "attach",
			"protocol": "inspector",
			"restart": true,
			"sourceMaps": true,
			"localRoot": "${workspaceFolder}",
			"remoteRoot": "/opt/app",
			"address": "localhost",
			"port": 9229,
			"resolveSourceMapLocations": ["${workspaceFolder}/dist/**"],
			"trace": true
		},
		{
			"type": "node",
			"request": "attach",
			"name": "Attach Radio WS",
			"port": 9230,
			"restart": true,
			"sourceMaps": true
		}
	]
}
